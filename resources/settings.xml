        <settingConfig type="list" required="true">
	    	    <listEntry value="0">@Strings.fetchModeNormal</listEntry>
	            <listEntry value="1">@Strings.fetchModeNormalMin</listEntry>
	            <listEntry value="2">@Strings.fetchModeDevice</listEntry>
	            <listEntry value="3">@Strings.fetchModeDeviceMin1</listEntry>
	            <listEntry value="4">@Strings.fetchModeDeviceMin2</listEntry>
	</settingConfig></setting>
    <setting propertyKey="@Properties.displaySeconds" title="@Strings.displaySeconds" prompt="@Strings.displaySecondsPrompt">
        <settingConfig type="boolean" required="true" /></setting>
	<setting propertyKey="@Properties.bgDisplay" title='@Strings.bgDisplay' prompt="@Strings.bgDisplayPrompt">
	    	<settingConfig type="list" required="true">
	    	    <listEntry value="0">@Strings.bgDisplayNormal</listEntry>
	            <listEntry value="1">@Strings.bgDisplayMGDL</listEntry>
	            <listEntry value="2">@Strings.bgDisplayMMOL</listEntry>
	            <listEntry value="3">@Strings.bgDisplayRoman</listEntry>
 		        <listEntry value="4">@Strings.bgDisplayHEX</listEntry>
<!--            <listEntry value="5">@Strings.bgDisplayHex</listEntry>        -->
