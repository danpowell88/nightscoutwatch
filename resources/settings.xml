	            <listEntry value="13">@Strings.fgCOLOR_PINK</listEntry>
	            <listEntry value="14">@Strings.fgCOLOR_RANDOM</listEntry>
	</settingConfig></setting>
	<setting propertyKey="@Properties.bgColor" title='@Strings.bgColor' prompt="@Strings.bgColorPrompt">
	    	<settingConfig type="list" required="true">
	            <listEntry value="0">@Strings.fgCOLOR_WHITE</listEntry>
	            <listEntry value="1">@Strings.fgCOLOR_LT_GRAY</listEntry>
	            <listEntry value="2">@Strings.fgCOLOR_DK_GRAY</listEntry>
	            <listEntry value="3">@Strings.fgCOLOR_BLACK</listEntry>
	            <listEntry value="4">@Strings.fgCOLOR_RED</listEntry>
	            <listEntry value="5">@Strings.fgCOLOR_DK_RED</listEntry>
	            <listEntry value="6">@Strings.fgCOLOR_ORANGE</listEntry>
	            <listEntry value="7">@Strings.fgCOLOR_YELLOW</listEntry>
	            <listEntry value="8">@Strings.fgCOLOR_GREEN</listEntry>
	            <listEntry value="9">@Strings.fgCOLOR_DK_GREEN</listEntry>
	            <listEntry value="10">@Strings.fgCOLOR_BLUE</listEntry>
	            <listEntry value="11">@Strings.fgCOLOR_DK_BLUE</listEntry>
	            <listEntry value="12">@Strings.fgCOLOR_PURPLE</listEntry>
	            <listEntry value="13">@Strings.fgCOLOR_PINK</listEntry>
	            <listEntry value="14">@Strings.fgCOLOR_RANDOM</listEntry>
