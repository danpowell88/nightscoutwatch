<settings>
    <setting propertyKey="@Properties.nsurl" title="@Strings.nsurl" prompt="@Strings.nsurlPrompt">
        <settingConfig type="alphanumeric" required="false" /></setting>
    <setting propertyKey="@Properties.offlineUrl" title="@Strings.offlineUrl" prompt="@Strings.offlineUrlPrompt">
        <settingConfig type="alphanumeric" required="false" /></setting>
